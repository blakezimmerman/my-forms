{
  "name": "my-forms",
  "version": "1.0.0",
  "description": "Project for Engineering Design VI",
  "author": "Blake Zimmerman <blake.zimmerman@me.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/blakezimmerman/my-forms.git"
  },
  "main": "src/server/app.ts",
  "scripts": {
    "start": "nodemon --watch 'src/server/**/*.ts' --exec 'npm run ts-node' src/server/app.ts",
    "start:prod": "NODE_ENV=prod node ./dist/server/server.bundle.js",
    "build:all": "npm run build:server && npm run build:client",
    "build:server": "npm run clean:server && webpack --config webpack/webpack.server.js --bail",
    "build:client": "npm run clean:client && webpack --config webpack/webpack.prod.js --bail",
    "docker:prod": "docker-compose up --build",
    "clean:client": "rimraf dist/client",
    "clean:server": "rimraf dist/server && rimraf dist/node_modules",
    "clean:mongo": "rimraf data",
    "clean:all": "npm run clean:client && npm run clean:server && npm run clean:mongo",
    "docker": "docker-compose -f dev.docker-compose.yml up --build",
    "deploy": "heroku container:push web -a myforms",
    "lint": "tslint \"src/**/*.ts*\"",
    "ts-node": "ts-node -O '{\"module\": \"commonjs\"}'"
  },
  "engines": {
    "node": ">= 9.3.0",
    "npm": ">= 5.6.0"
  },
  "dependencies": {
    "@types/ramda": "^0.25.21",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
